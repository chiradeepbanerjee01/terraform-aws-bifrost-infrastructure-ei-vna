# MAKE SURE TO RENAME THE FILE TO REMOVE THE `.template` PRIOR TO ADDING YOUR KEYS #
# IF NECESSARY `git rm terraform.tfvars` TO ENSURE YOU ARE NOT COMMITTING YOUR KEYS TO THE REPO #
access_key = "<<ACCESS_KEY>>"
secret_key = "<<SECRET_KEY>>"
assume_role = "arn:aws:iam::<<ACCOUNT_NUMBER>>:role/@iac_deploy_role"
zone_name = "<<ENV_TYPE>>.<<PROGRAM_KEY>>.<<BDU>>.bdu.hyland.dev" # where ENV_TYPE is Sandbox, Dev, QA, Etc. NO PRODUCTION WORKLOADS ARE ALLOWED IN CPE'S DELIVERY ORG
region = "us-east-2"